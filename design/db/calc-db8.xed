   <View id="51" name="plot_section_view" schema="calc" database="calc" background="15790335">
      <Text>Observed plot sections and respective sampling design attributes</Text>
      <Statement>select     
    p.*,
    ps.id as plot_section_id,
    ps.section as plot_section,
    ps.visit_type,
    ps.survey_date,
    ps.gps_reading,
    ps.direction,
    ps.distance,
    ps.location as survey_location,
    ps.accessible,
    ps.step,
    ps.shape as plot_survey_shape,
    ps.area as plot_survey_area,
    ps.percent_share
from "calc"."plot_section" ps
inner join "calc"."ground_plot_view" p
    on ps.sample_plot_id = p.sample_plot_id
</Statement>
   </View>
