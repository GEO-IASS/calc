   <View id="95" name="plot_survey_view" schema="calc" database="calc" background="15790335">
      <Text>Observed plot sections and respective sampling design attributes</Text>
      <Statement>select     
    p.*,
    ps.id as plot_survey_id,
    ps.section_no as plot_section,
    ps.survey_date,
    ps.gps_reading,
    ps.direction,
    ps.distance,
    ps.location as survey_location,
    ps.accessible,
    ps.parent_id as plot_survey_parent_id,
    ps.survey_type,
    ps.step,
    ps.shape as plot_survey_shape,
    ps.area as plot_survey_area,
    ps.percent_share,
    u.id as obs_unit_id,
    u.name as obs_unit_name
from "calc"."plot_survey" ps
inner join "calc"."ground_plot_view" p
    on ps.plot_id = p.plot_id
inner join "calc"."observation_unit" u 
    on ps.obs_unit_id = u.id</Statement>
   </View>
