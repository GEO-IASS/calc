   <View id="178" name="sample_plot_view" schema="calc" database="calc" background="15790335">
      <Text>Denormalized sampling design </Text>
      <Statement>select     
    u.survey_id,
    p.obs_unit_id,
    u.name as obs_unit_name,
    str.id as stratum_id,
    str.no as stratum_no,
    c.id as cluster_id,
    c.code as cluster_code,
    c.x_index as cluster_x,
    c.y_index as cluster_y,
    p.id as sample_plot_id,
    p.no as sample_plot_no,
    p.location,
    p.shape,
    p.phase,
    p.ground_plot,
    p.permanent_plot
from "calc"."sample_plot" p
inner join "calc"."cluster" c
    on p.cluster_id = c.id
inner join "calc"."stratum" str
    on p.stratum_id = str.id
inner join "calc"."observation_unit" u
    on p.obs_unit_id = u.id</Statement>
   </View>
   <View id="179" name="plot_measurement_view" schema="calc" database="calc" background="15790335">
      <Text>Plot measurements and related variable names and plot, cluster and stratum codes</Text>
      <Statement>select     
    ps.*,
    v.name as variable_name,
    pm.value,
    pm.computed
from "calc"."plot_measurement" pm
inner join "calc"."plot_section_view" ps
    on pm.plot_section_id = ps.plot_section_id
inner join "calc"."variable" v
    on pm.variable_id = v.id
</Statement>
   </View>
   <View id="180" name="plot_category_view" schema="calc" database="calc" background="15790335">
      <Text>Plot categories and related codes, variable names and plot, cluster and stratum codes</Text>
      <Statement>select     
    pc.*,
    cat.variable_id,
    cat.variable_type,
    cat.variable_name,
    cat.variable_order,
    cat.category_code,
    cat.category_order
from "calc"."plot_category" pc
inner join "calc"."category_view" cat
    on pc.category_id = cat.category_id</Statement>
   </View>
   <View id="181" name="ground_plot_view" schema="calc" database="calc" background="15790335">
      <Text>All sampling design plots designated for in situ observations</Text>
      <Statement>select     
    *
from sample_plot_view 
where ground_plot
</Statement>
   </View>
   <View id="182" name="category_view" schema="calc" database="calc" background="15790335">
      <Statement>select     
    v.id as variable_id,
    v.type as variable_type,
    v.name as variable_name,
    v.order as variable_order,
    cat.code as category_code,
    cat.order as category_order
from "calc"."category" cat
inner join "calc"."variable" v
    on cat.variable_id = v.id</Statement>
   </View>
