insert into calc.specimen_categorical_value (specimen_id,  category_id, original, current)
select
    s.specimen_id,
    case 
        when t.taxon_code like 'ADA%' then 1495
        else 1496
    end
    as cat_id,
    true,
    true
from
    calc.specimen s
inner join
    calc.taxon t
on
    (
        s.specimen_taxon_id = t.taxon_id) 
        